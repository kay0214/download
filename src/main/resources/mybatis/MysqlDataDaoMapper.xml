<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="com.sandman.download.dao.mysql.MysqlDataDao">
	<select id="seletTestInfoById" parameterType="Integer" resultType="HashMap">
		select * from test where id = #{_parameter}
	</select>
	<update id="replaceTestInfoById" parameterType="HashMap">
		REPLACE INTO test(id,name) VALUES (#{id},#{name});
	</update>
</mapper>